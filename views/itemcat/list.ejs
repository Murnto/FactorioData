<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../layout/head.ejs %>
</head>
<body>
<% include ../layout/header.ejs %>
<div class="container">
    <h2><%= title %></h2>
    <table class="sortable-theme-bootstrap" data-sortable id="table">
        <thead>
            <tr>
                <th>Icon</th>
                <th>Name</th>
<%
    for (var i = 0; i < headers.length; i++) {
%>
                <th><%= headers[i] %></th>
<% } %>
            </tr>
        </thead>
        <tbody>
<%
    for (var i = 0; i < columnData.length; i++) {
        var item = columnData[i];
%>
        <tr>
            <td><%- embedImg(item.type, item.name) %></td>
            <td><a href="<%= itemUrl(item.type, item.name) %>"><%= item.title || item.name %></a></td>
<%
for (var j = 0; j < headers.length; j++) {
%>
            <td><%= item[j] %></td>
<% } %>
        </tr>
<% } %>
        </tbody>
    </table>
</div>

<footer>
    <% include ../layout/footer.ejs %>
</footer>

</body>
</html>
